  "scripts": {
    "build-main": "cross-env NODE_ENV=production node --max-old-space-size=2024 -r babel-register ./node_modules/webpack/bin/webpack --config ./webpackConfigs/ui/webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max-old-space-size=2024 -r babel-register ./node_modules/webpack/bin/webpack --config ./webpackConfigs/ui/webpack.config.production.js --progress --profile --colors",
    "download-externals": "babel-node tools/downloadExternals",
    "download-externals-all": "babel-node tools/downloadExternals --all",
    "dev-ui": "cross-env HOT=1 NODE_ENV=development node --max-old-space-size=2024 -r babel-register ./node_modules/.bin/webpack-dev-server --config ./webpackConfigs/ui/webpack.config.js --port 9000 --content-base ./wail-ui/ --hot --inline",
    "dev-ui-electron": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./wail-ui/ui-main.js",
    "nightmare": "babel-node tools/testNightmare",
    "standard": "standard --verbose | snazzy",
    "test-xml": "babel-node tools/test-xml",
    "package": "cross-env NODE_ENV=production node -r babel-register build-binary.js",
    "package-osx": "npm run package -- --osx",
    "standard-fix": "standard --fix"
  },
  "main": "./dist/ui-main.js",
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
