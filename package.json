  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "es6": true
    },
    "globals": [
      "React",
      "window"
    ],
    "rules": {
      "comma-dangle": "off",
      "react/jsx-boolean-value": "off",
      "react/jsx-space-before-closing": "off",
      "no-unused-var": "off",
      "no-undef": "off"
    },
    "extends": [
      "standard",
      "standard-react"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "archiveIndexes/*",
      "archives/*",
      "buildResources/*",
      "bundledApps",
      "config/*",
      "*.cxml",
      "dist/*",
      "Dockerfile",
      "LICENSE",
      "*.log",
      "*.md",
      "node_modules/*",
      "package.js",
      "release/*",
      "*.sh",
      "support/*",
      "test/*",
      "tools/*",
      "*.txt",
      "waillogs/*",
      "webpack.*",
      "*.xml",
      "*.yml",
      "zips/*",
      "images/*",
      "memgators/*",
      ""
    ],
    "globals": [
      "React",
      "window"
    ],
    "env": {
      "node": true,
      "browser": true,
      "es6": true
