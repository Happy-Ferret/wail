    "download-externals": "babel-node tools/downloadExternals",
    "download-externals-all": "babel-node tools/downloadExternals --all",
    "dev-ui": "node -r babel-register ./node_modules/.bin/webpack-dev-server --config ./webpackConfigs/ui/webpack.config.js --port 9000 --content-base ./wail-ui/ --hot --inline",
    "dev-ui-electron": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./wail-ui/ui-main",
    "dev-core-electron": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./wail-core/core-main.js",
    "dev-core": "cross-env node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpackConfigs/core/webpack.config.js --port 9000 --content-base ./wail-core/ --hot --inline",
    "core-server": "babel-node wail-core/server",
    "nightmare": "babel-node tools/testNightmare",
    "standard": "standard --verbose | snazzy",
    "test-xml": "babel-node tools/test-xml"
